{# src/CoreBundle/Ressources/views/index.html.twig #}

<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>
    {% block title %}
      Family's Team
    {% endblock %}
  </title>

  {% block stylesheets %}
      {% stylesheets filter='cssrewrite, ?scssphp'
      'bundles/altgeekcore/css/CoreMain.css' %}
        <link rel="stylesheet" href="{{ asset('bundles/altgeekcore/css/coreMain.css') }}" type="text/css" />
      {% endstylesheets %}
  {% endblock %}

</head>

<body>
  <div class="container">

    {% block header %}
      {{ include("AltgeekCoreBundle:Core:header.html.twig") }}
    {% endblock %}

	<div id="content" class="col-md-9">
		{% block body %}
		<p> {{ 'Homepage content'|trans }} </p>
		{% endblock %}
	</div>

  {% block footer %}
    {{ include("AltgeekCoreBundle:Core:footer.html.twig") }}
  {% endblock %}

  {% block javascripts %}
    {% javascripts filter='?jsqueeze'
      'bundles/altgeekcore/js/CoreMain.js' %}
      <script type="text/javascript" src="{{ asset('bundles/altgeekcore/js/coreMain.js') }}"></script>
    {% endjavascripts %}
  {% endblock %}

</body>
</html>